//33. WAPC to display the first n natural numbers. here 'n' is the user input.

#include<stdio.h>

int main()
{
    int n, i;

    printf("\nEnter the value of n: ");
    scanf("%d", &n);

    printf("\nThe first %d natural numbers are:\n", n);

    for(i = 1; i <= n; i++)
    {
        printf("%d ", i);
    }

    return 0;
}